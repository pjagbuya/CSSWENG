<div class="form">
    <div class="form-header">
        <div class="form-logo">
            <img class="logo" src="./files/Logo.png">
        </div>
        <div class="form-details">
            {{#with formDetail as |formDetail|}}
                <table>
                    <tr>
                        <td>
                            <b>Google Developers Student Clubs</b>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <b>De La Salle University</b>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            {{formDetail.academicYear}}
                        </td>
                    </tr>
                    <tr>
                        <td>
                            
                        </td>
                    </tr>
                    <tr>
                        <td>
                            {{formDetail.formType}}
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <table>
                                <tr>
                                    <td>
                                        Report Code:
                                    </td>
                                    <td>
                                        {{formDetail.formCode}}
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            {{/with}}
        </div>
    </div>
    <div class="form-main">
        {{#with form as |form|}}
            <div class=""
        {{/with}}
    </div>
    <div class="form-footer">
        {{#each staff as |staff|}}
            <div class="staff-signature">
                {{staff.message}}
                <div class="line">
                </div>
                <div class="staff-details">
                    {{staff.name}}
                    {{staff.position}}
                </div>
            </div>
        {{/each}}
    </div>
</div>